  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DS_AdventrueWorks</DataSourceName>
        <CommandText>
SELECT 
    DATEFROMPARTS(YEAR(OrderDate), MONTH(OrderDate), 1) AS Month,
    SUM(TotalDue) AS MonthlyRevenue
FROM Sales.SalesOrderHeader
GROUP BY DATEFROMPARTS(YEAR(OrderDate), MONTH(OrderDate), 1)
ORDER BY Month;</CommandText>
      </Query>
      <Fields>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="MonthlyRevenue">
          <DataField>MonthlyRevenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
