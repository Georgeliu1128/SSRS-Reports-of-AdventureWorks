  <DataSets>
    <DataSet Name="DS_MarginByCategory">
      <Query>
        <DataSourceName>MarginByCategory</DataSourceName>
        <CommandText>SELECT 
    pc.Name AS ProductCategory,
    p.ListPrice,
    p.StandardCost,
    (p.ListPrice - p.StandardCost) AS Profit
FROM Production.Product p
JOIN Production.ProductSubcategory ps ON p.ProductSubcategoryID = ps.ProductSubcategoryID
JOIN Production.ProductCategory pc ON ps.ProductCategoryID = pc.ProductCategoryID
WHERE p.ListPrice &gt; 0 AND p.StandardCost &gt; 0
</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductCategory">
          <DataField>ProductCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ListPrice">
          <DataField>ListPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="StandardCost">
          <DataField>StandardCost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Profit">
          <DataField>Profit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
