  <DataSets>
    <DataSet Name="DS_ProductShortage">
      <Query>
        <DataSourceName>ProductShortage</DataSourceName>
        <CommandText>SELECT 
    p.Name AS ProductName,
    i.Quantity AS CurrentStock,
    p.SafetyStockLevel,
    (p.SafetyStockLevel - i.Quantity) AS Shortage
FROM Production.Product p
JOIN Production.ProductInventory i ON p.ProductID = i.ProductID
WHERE i.Quantity &lt; p.SafetyStockLevel
ORDER BY Shortage DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CurrentStock">
          <DataField>CurrentStock</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="SafetyStockLevel">
          <DataField>SafetyStockLevel</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="Shortage">
          <DataField>Shortage</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
